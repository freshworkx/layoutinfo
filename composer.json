    "type": "typo3-cms-extension",
    "description": "TYPO3 CMS extension that shows the active backend layout for the current page in page module",
    "license": [
        "GPL-2.0-or-later"
    ],
    "authors": [
        {
            "name": "Andreas Kiefer",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms-core": "^9.5 || ^10.4"
    },
    "autoload": {
        "psr-4": {
            "Kiefer\\LayoutInfo\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kiefer\\LayoutInfo\\Tests\\": "Tests"
        }
    }
}
